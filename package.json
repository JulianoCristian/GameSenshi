{
	"name": "game-senshi",
	"version": "1.0.0",
	"description": "Hire Your Favorite Player!",
	"private": true,
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zero-to-mastery/GameSenshi.git"
	},
	"keywords": [
		"react",
		"PWA",
		"Firebase",
		"Game Senshi",
		"social",
		"gaming",
		"platform",
		"service",
		"bootstrap"
	],
	"author": "tylim",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/zero-to-mastery/GameSenshi/issues"
	},
	"scripts": {
		"divider0": "---------------------------------------------utilities------------------------------------------------------------",
		"lint": "eslint src/ --fix",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm i",
		"compile-sass": "node-sass src/0_assets/scss/blk-design-system-pro-react.scss src/0_assets/css/blk-design-system-pro-react.css",
		"minify-sass": "node-sass src/0_assets/scss/blk-design-system-pro-react.scss src/0_assets/css/blk-design-system-pro-react.min.css --output-style compressed",
		"map-sass": "node-sass src/0_assets/scss/blk-design-system-pro-react.scss src/0_assets/css/blk-design-system-pro-react.css --source-map true",
		"divider1": "---------------------------------------------basic----------------------------------------------------------------",
		"start": "react-scripts start",
		"build": "react-scripts build && node src/sw-build.js",
		"deploy": "npm run lint && npm run build && cross-env-shell \"firebase use $REACT_APP_PROJECT_ID\" && firebase deploy --only hosting",
		"fs-rule": "cross-env-shell \"firebase use $REACT_APP_PROJECT_ID\" && firebase deploy --only firestore:rules",
		"divider2": "-----------------------------------------------dev-----------------------------------------------------------------",
		"dev": "env-cmd .config/dev.env npm run start",
		"d-build": "env-cmd .config/dev.env npm run build",
		"d-deploy": "env-cmd .config/dev.env npm run deploy",
		"d-fs-rule": "env-cmd .config/dev.env npm run fs-rule",
		"divider3": "-----------------------------------------------prod----------------------------------------------------------------",
		"prod": "env-cmd .config/prod.env npm run start",
		"p-build": "env-cmd .config/prod.env npm run build",
		"p-deploy": "env-cmd .config/prod.env npm run deploy",
		"p-fs-rule": "env-cmd .config/prod.env npm run fs-rule",
		"divider4": "-------------------------------------------------all-----------------------------------------------------------------",
		"a-deploy": "npm run d-deploy && npm run p-deploy"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.11.2",
		"@material-ui/core": "^4.5.1",
		"@material-ui/icons": "^4.5.1",
		"@material-ui/lab": "^4.0.0-alpha.29",
		"apollo-cache-inmemory": "^1.6.2",
		"apollo-cache-persist": "^0.1.1",
		"apollo-client": "^2.6.3",
		"apollo-link-http": "^1.5.16",
		"apollo-link-persisted-queries": "^0.2.2",
		"array-shuffle": "^1.0.1",
		"card-validator": "^6.2.0",
		"chroma-js": "^2.0.4",
		"clsx": "^1.1.0",
		"country-list": "^2.1.1",
		"final-form": "^4.12.0",
		"final-form-focus": "^1.1.2",
		"firebase": "^7.6.1",
		"fslightbox-react": "^1.4.7",
		"graphql": "^14.5.6",
		"graphql-tag": "^2.10.1",
		"history": "^4.10.1",
		"interweave": "^11.1.0",
		"localforage": "^1.7.3",
		"material-ui-image": "^3.2.2",
		"moment": "^2.24.0",
		"payment-icons": "^1.2.1",
		"react": "^16.8.3",
		"react-apollo": "^2.5.8",
		"react-credit-cards": "^0.7.0",
		"react-cropper": "^1.3.0",
		"react-datetime": "^2.16.3",
		"react-dom": "^16.8.3",
		"react-element-to-jsx-string": "^14.1.0",
		"react-final-form": "^5.0.2",
		"react-helmet": "^5.2.1",
		"react-image-gallery": "^0.9.1",
		"react-loader-spinner": "^2.3.0",
		"react-notification-alert": "0.0.12",
		"react-perfect-scrollbar": "^1.5.3",
		"react-player": "^1.13.0",
		"react-resize-detector": "^4.2.1",
		"react-router-dom": "^5.1.2",
		"react-router-last-location": "^2.0.0-beta.3",
		"react-router-link-nostack": "^1.0.5",
		"react-scripts": "^2.1.5",
		"react-select": "^2.4.3",
		"react-slick": "^0.25.2",
		"react-sound": "^1.2.0",
		"react-tagsinput": "^3.19.0",
		"reactstrap": "^8.1.1",
		"shorthash": "0.0.2",
		"slick-carousel": "^1.8.1",
		"styled-break": "^2.1.0",
		"styled-components": "^4.4.1",
		"unstated": "^2.1.1",
		"uuid": "^3.3.3",
		"yup": "^0.27.0"
	},
	"devDependencies": {
		"cross-env": "^5.2.0",
		"env-cmd": "^8.0.2",
		"eslint-config-prettier": "^4.1.0",
		"eslint-plugin-prettier": "^3.1.1",
		"husky": "^4.0.0-beta.1",
		"node-sass": "^4.13.1",
		"prettier": "^1.16.4",
		"pretty-quick": "^1.10.0",
		"workbox-build": "^4.3.1"
	},
	"prettier": {
		"eslintIntegration": true,
		"singleQuote": true,
		"jsxSingleQuote": true,
		"trailingComma": "es5",
		"semi": false,
		"jsxBracketSameLine": false,
		"useTabs": true
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"prettier/react",
			"eslint:recommended",
			"plugin:prettier/recommended",
			"prettier"
		],
		"rules": {
			"no-console": "warn",
			"no-unused-vars": [
				"error",
				{
					"args": "after-used"
				}
			]
		},
		"settings": {
			"import/resolver": {
				"node": {
					"moduleDirectory": [
						"node_modules",
						"src/"
					]
				}
			}
		}
	},
	"eslintIgnore": [
		"build/*",
		".config/*",
		"functions/*",
		".github/*",
		".firebase/*",
		"src/serviceWorker.js",
		"src/sw-build.js",
		"src/sw-template.js"
	],
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"development": [
		"last 1 chrome version",
		"last 1 firefox version",
		"last 1 safari version"
	]
}
